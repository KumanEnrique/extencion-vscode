{
  "name": "my-first-extension",
  "displayName": "my-first-extension",
  "description": "crear un generador de objetos, como emmet pero en vez de html hacerlo en js o php",
  "version": "0.0.1",
  "publisher": "lkuman6",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:my-first-extension.helloWorld",
    "onCommand:my-first-extension.repetirtexto",
    "onCommand:my-first-extension.folderStruct",
    "onCommand:my-first-extension.mvvmFolderGenerate"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-first-extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "my-first-extension.repetirtexto",
        "title": "Repetir texto"
      },
      {
        "command": "my-first-extension.folderStruct",
        "title": "estructura propia MVC php"
      },
      {
				"command": "my-first-extension.mvvmFolderGenerate",
				"title": "[normal] MVVM Folder Generate"
			}
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "my-first-extension.folderStruct",
          "group": "estructura propia MVC php"
        },
        {
					"group": "2_workspace",
					"command": "my-first-extension.mvvmFolderGenerate"
				}
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
